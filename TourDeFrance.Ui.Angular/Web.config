<?xml version="1.0"?>
<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.5"/>
    <httpRuntime targetFramework="4.5"/>
    <httpModules/>
  </system.web>
  <system.webServer>
    <rewrite>
      <rules>
        <rule name="Reverse Proxy to API" stopProcessing="true">
          <match url="^api/(.*)"/>
          <action type="Rewrite" url="http://localhost:12340/api/{R:1}" logRewrittenUrl="true"/>
        </rule>
      </rules>
    </rewrite>
  </system.webServer>
</configuration>